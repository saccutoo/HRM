@model List<string>
<div class="presentation-name">
    @if (Model.Count > 0)
    {
        var index = 0;
        foreach (var item in Model)
        {
            var itemLast = Model.Count() - 1;
            if (index == 0)
            {
                <div class="first-row">
                    <a href="@Model[itemLast]">@item</a>
                </div>
            }
            else if (index != itemLast)
            {
                <div class="normal-row">
                    @item
                </div>
            }
            index++;
        }
    }
</div>


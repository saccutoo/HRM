@using Hrm.Framework.Models
@using Hrm.Framework.Helper
@model List<Hrm.Framework.Models.WorkingdayCheckInOutModel>

<ul>
    <li>@T("WorkingdaySummary.HistoryCheckinOut")</li>
    @if (Model.Count()>0)
    {
        foreach (var item in Model)
        {
            <li>@item.CheckTime.ToString("HH:mm")</li>
        }
    }
</ul>